{
  "extends": "@vue/tsconfig/tsconfig.json",//@ts-ignore
  "include": [
    "types/auto-imports.d.ts",
    "types/components.d.ts",
    "env.d.ts",
    "types/typed-router.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["unplugin-icons/types/vue"],
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmitOnError": false,
  }
}
